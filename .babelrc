{
    "presets": [
        [
            "next/babel",
            {
                "styled-jsx": {
                    "plugins": [
                        "styled-jsx-plugin-sass"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd"
            }
        ],
        [
            "styled-components",
            {
                "ssr": true
            }
        ]
    ]
}